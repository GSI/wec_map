					<state>
						<TCEforms>
							<label>LLL:EXT:wec_map/locallang_db.xml:tt_content.pi_flexform.state</label>
							<config>
								<type>input</type>
								<size>30</size>
							</config>
						</TCEforms>
					</state>
					
					<zip>
						<TCEforms>
							<label>LLL:EXT:wec_map/locallang_db.xml:tt_content.pi_flexform.zip</label>
							<config>
								<type>input</type>
								<size>10</size>
							</config>
						</TCEforms>
					</zip>
					
					<country>
						<TCEforms>
							<label>LLL:EXT:wec_map/locallang_db.xml:tt_content.pi_flexform.country</label>
							<config>
								<type>input</type>
								<size>20</size>
