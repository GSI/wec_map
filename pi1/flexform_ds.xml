						<TCEforms>
							<label>LLL:EXT:wec_map/locallang_db.xml:tt_content.pi_flexform.coord</label>
							<config>
								<type>user</type>
								<userFunc>tx_wecmap_backend->checkGeocodeStatusFF</userFunc>
							</config>
						</TCEforms>
					</coord>
				</el>
			</ROOT>
		</default>
		
		<mapConfig>
			<ROOT>
				<TCEforms>
					<sheetTitle>LLL:EXT:wec_map/locallang_db.xml:tt_content.pi_flexform.mapTitle</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
					<apiKey>
						<TCEforms>
							<label>LLL:EXT:wec_map/locallang_db.xml:tt_content.pi_flexform.apiKey</label>
							<config>
								<type>input</type>
								<size>50</size>
							</config>
						</TCEforms>
					</apiKey>
						
					<mapWidth>
						<TCEforms>
							<label>LLL:EXT:wec_map/locallang_db.xml:tt_content.pi_flexform.mapWidth</label>
