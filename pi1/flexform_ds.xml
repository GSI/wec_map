									<numIndex index="1" type="array">
										<numIndex index="0">LLL:EXT:wec_map/locallang_db.xml:tt_content.pi_flexform.typeSatellite</numIndex>
										<numIndex index="1">G_SATELLITE_MAP</numIndex>
